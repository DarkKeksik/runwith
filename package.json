    "cross-env": "^7.0.3",
    "mini-css-extract-plugin": "^1.3.6",
    "node-sass": "^6.0.1",
    "nodemon": "^2.0.7",
    "null-loader": "^4.0.1",
    "sass-loader": "^12.3.0",
    "ts-loader": "^8.0.17",
    "typescript": "^4.1.5",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "css-loader": "^5.0.2",
